<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>Description</th>
      <th>start_date</th>
      <th>end_date</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
    <tr>
      <td><%= event.title %></td>
      <td><%= event.description %></td>
      <td><%= event.start_date %></td>
      <td><%= event.end_date %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<div id="calendar"></div>
